{
  "manifest_version": 2,

  "name": "JS Beautifier",
  "description": "Easy access",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "clipboardRead",
   "contextMenus",
    "tabs",
    "clipboardWrite"
   ]
}
